<h1>Sign in</h1>

<%= form_with(url: sign_in_path, method: :post) do %>
  <%= label_tag :username %><br>
  <%= text_field_tag :username, nil, required: true %>

  <br>

  <%= label_tag :password %><br>
  <%= password_field_tag :password, nil, required: true %>

  <br>

  <%= submit_tag "Sign in" %>
<% end %>